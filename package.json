{
  "name": "@resoc/netlify-plugin-social-image",
  "version": "0.0.1",
  "description": "Netlify Build plugin - {{description}}",
  "main": "src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.sh",
    "src/**/*.html",
    "src/**/*.ejs",
    "manifest.yml"
  ],
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "{{author}} <{{email}}>",
  "license": "{{license}}",
  "repository": "{{repo}}",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "build": "netlify-build",
    "test": "npm run lint && npm run ava",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"{src,init}/**/*.js\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"{.github,src,init}/**/*.{js,md,yml,json}\" \"*.{js,md,yml,json}\"",
    "ava": "cross-env FORCE_COLOR=1 ava --verbose",
    "release": "release-it"
  },
  "devDependencies": {
    "@netlify/build": "^18.8.0",
    "ava": "^3.13.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.0.0",
    "netlify-cli": "^6.8.13",
    "prettier": "^2.1.2",
    "release-it": "^14.0.3"
  },
  "dependencies": {
    "@resoc/core": "^0.5.0",
    "@resoc/create-img": "^0.5.2",
    "chrome-aws-lambda": "^10.1.0",
    "fs-extra": "^10.0.0",
    "puppeteer-core": "^10.2.0"
  }
}
